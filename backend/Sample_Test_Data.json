  "testScenarios": {
    "scenario1": {
      "name": "New User Registration and Login",
      "steps": [
        {
          "step": 1,
          "action": "POST /api/auth/register",
          "data": {
            "name": "Test User",
            "email": "testuser@example.com",
            "password": "testpass123",
            "budgetRange": {
              "min": 500,
              "max": 2000
            },
            "usageType": "work"
          }
        },
        {
          "step": 2,
          "action": "POST /api/auth/login",
          "data": {
            "email": "testuser@example.com",
            "password": "testpass123"
          },
          "expectedResult": "Returns JWT token"
        }
      ]
    },
    "scenario2": {
      "name": "Price Prediction for Different Laptop Types",
      "steps": [
        {
          "step": 1,
          "action": "POST /api/predict",
          "description": "Predict high-end gaming laptop price",
          "data": {
            "ram": 16,
            "weight": 2.0,
            "company": "Dell",
            "typename": "Gaming",
            "opsys": "Windows",
            "cpu": "Intel Core i9",
            "gpu": "NVIDIA RTX 4080",
            "touchscreen": true,
            "ips": true
          }
        },
        {
          "step": 2,
          "action": "POST /api/predict",
          "description": "Predict budget laptop price",
          "data": {
            "ram": 4,
            "weight": 1.8,
            "company": "Lenovo",
            "typename": "Budget",
            "opsys": "Windows",
            "cpu": "AMD Ryzen 3",
            "gpu": "AMD Radeon",
            "touchscreen": false,
            "ips": false
          }
        },
        {
          "step": 3,
          "action": "GET /api/predict/history",
          "expectedResult": "Returns all predictions for the user"
        }
      ]
    },
    "scenario3": {
      "name": "Get Laptop Recommendations",
      "steps": [
        {
          "step": 1,
          "action": "GET /api/recommend/personalized",
          "expectedResult": "Returns personalized recommendations based on user profile"
        },
        {
          "step": 2,
          "action": "GET /api/recommend/collaborative",
          "expectedResult": "Returns collaborative filtering recommendations"
        },
        {
          "step": 3,
          "action": "POST /api/recommend/track",
          "data": {
            "laptopId": "LAPTOP_ID",
            "interactionType": "view"
          },
          "expectedResult": "Tracks user interaction for recommendation improvement"
        }
      ]
    },
    "scenario4": {
      "name": "User Profile Management",
      "steps": [
        {
          "step": 1,
          "action": "GET /api/auth/me",
          "expectedResult": "Returns current user profile"
        },
        {
          "step": 2,
          "action": "PUT /api/auth/preferences",
          "data": {
            "budgetRange": {
              "min": 1000,
              "max": 3000
            },
            "usageType": "gaming"
          },
          "expectedResult": "Updates user preferences"
        },
        {
          "step": 3,
          "action": "GET /api/users/dashboard",
          "expectedResult": "Returns user dashboard with statistics"
        }
      ]
    },
    "scenario5": {
      "name": "Submit Prediction Feedback",
      "steps": [
        {
          "step": 1,
          "action": "POST /api/predict/feedback",
          "data": {
            "predictionId": "PREDICTION_ID",
            "accuracy": 5,
            "comment": "Excellent prediction! Very accurate."
          },
          "expectedResult": "Feedback submitted and stored"
        }
      ]
    }
  },
  "apiEndpointsReference": {
    "baseURL": "http://localhost:5001",
    "endpoints": {
      "authentication": [
        {
          "method": "POST",
          "endpoint": "/api/auth/register",
          "description": "Register a new user",
          "requiresAuth": false
        },
        {
          "method": "POST",
          "endpoint": "/api/auth/login",
          "description": "Login user and get JWT token",
          "requiresAuth": false
        },
        {
          "method": "GET",
          "endpoint": "/api/auth/me",
          "description": "Get current user profile",
          "requiresAuth": true
        },
        {
          "method": "PUT",
          "endpoint": "/api/auth/profile",
          "description": "Update user profile",
          "requiresAuth": true
        },
        {
          "method": "PUT",
          "endpoint": "/api/auth/preferences",
          "description": "Update user preferences",
          "requiresAuth": true
        }
      ],
      "prediction": [
        {
          "method": "POST",
          "endpoint": "/api/predict",
          "description": "Predict laptop price based on specifications",
          "requiresAuth": false
        },
        {
          "method": "GET",
          "endpoint": "/api/predict/history",
          "description": "Get user's prediction history",
          "requiresAuth": true
        },
        {
          "method": "POST",
          "endpoint": "/api/predict/feedback",
          "description": "Submit feedback on a prediction",
          "requiresAuth": true
        }
      ],
      "recommendations": [
        {
          "method": "GET",
          "endpoint": "/api/recommend/personalized",
          "description": "Get personalized recommendations",
          "requiresAuth": true
        },
        {
          "method": "GET",
          "endpoint": "/api/recommend/collaborative",
          "description": "Get collaborative filtering recommendations",
          "requiresAuth": true
        },
        {
          "method": "GET",
          "endpoint": "/api/recommend/similar/:laptopId",
          "description": "Get similar laptops to a specific one",
          "requiresAuth": true
        },
        {
          "method": "GET",
          "endpoint": "/api/recommend/content-based/:laptopId",
          "description": "Get content-based recommendations",
          "requiresAuth": true
        },
        {
          "method": "POST",
          "endpoint": "/api/recommend/track",
          "description": "Track user interaction with laptops",
          "requiresAuth": true
        }
      ],
      "userDashboard": [
        {
          "method": "GET",
          "endpoint": "/api/users/dashboard",
          "description": "Get user dashboard",
          "requiresAuth": true
        },
        {
          "method": "GET",
          "endpoint": "/api/users/activity",
          "description": "Get user activity logs",
          "requiresAuth": true
        },
        {
          "method": "PUT",
          "endpoint": "/api/users/settings",
          "description": "Update user settings",
          "requiresAuth": true
        },
        {
          "method": "GET",
          "endpoint": "/api/users/export",
          "description": "Export user data",
          "requiresAuth": true
        },
        {
          "method": "DELETE",
          "endpoint": "/api/users/account",
          "description": "Delete user account",
          "requiresAuth": true
        }
      ]
    }
  },
  "notes": {
    "authentication": "Include JWT token in Authorization header: Bearer YOUR_JWT_TOKEN",
    "contentType": "All requests should have Content-Type: application/json header",
    "baseURL": "Default base URL is http://localhost:5001",
    "predictionInputs": "RAM in GB, weight in kg, company name, type (Gaming/Ultrabook/Budget/etc), OS, CPU name, GPU name, boolean features",
    "currency": "Prices are in EUR by default, can be converted to other currencies",
    "limitedTokenAccess": "Some endpoints like /api/predict work without authentication (optional auth)",
    "errorHandling": "All endpoints return standard error responses with status codes and error messages"
  }
